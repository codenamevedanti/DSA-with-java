package com.demo.list;

public class MyLinkedList {
	    Node head;
        class Node{
        	int data;
        	Node next;
			public Node(int data, Node next) {
				super();
				this.data = data;
			    next=null;
			} 	
        }
		public MyLinkedList(Node head) {
			head = null;
		}
        
        
}
